					<key>source</key>
					<string>-- Insert Em Dash at Cursor-- Get the current applicationtell application "System Events"	set frontApp to name of first application process whose frontmost is trueend tell-- Check if the current application is a text editing applicationif frontApp is in {"TextEdit", "Microsoft Word", "Pages", "Notes", "TextEdit", "Mail"} then	tell application frontApp		-- Insert the em dash at the current text cursor position		set emDashUnicode to "â€”"		set the clipboard to emDashUnicode		delay 0.05		tell application "System Events" to keystroke "v" using {command down}	end tellelse	display dialog "This service works only in text editing applications like TextEdit, Microsoft Word, Pages, Notes, and Mail."end if</string>
